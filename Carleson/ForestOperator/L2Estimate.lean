private lemma K'_eq_K (b : ‚Ñ§) (c : ‚Ñ§) (x y : X)
    (h : dist x y ‚àà Icc ((D : ‚Ñù) ^ (b - 1) / 2) (D ^ c / 4)) : K' b c x y = K x y := by
  have hxy : dist x y > 0 := lt_of_lt_of_le (div_pos (defaultD_pow_pos a (b - 1)) two_pos) h.1
  simp_rw [K', Ks, ‚Üê Finset.mul_sum, ‚Üê Complex.ofReal_sum]
  rw [‚Üê finsum_eq_sum_of_support_subset, finsum_œà (one_lt_D (X := X)) hxy, ofReal_one, mul_one]
  intro i hi
  simp only [support_œàS (one_lt_D (X := X)) hxy, nonzeroS, Finset.coe_Icc, mem_Icc] at hi
  simp only [toFinset_Icc, Finset.coe_Icc, mem_Icc]
  refine ‚ü®le_trans ?_ hi.1, le_trans hi.2 ?_‚ü©
  ¬∑ rw [‚Üê Nat.cast_one, Int.floor_nat_add, Nat.cast_one, ‚Üê sub_le_iff_le_add', Int.le_floor,
      Real.le_logb_iff_rpow_le (one_lt_D (X := X)) (mul_pos two_pos hxy), mul_comm]
    exact_mod_cast (div_le_iff‚ÇÄ two_pos).mp h.1
  ¬∑ rw [Int.ceil_le, Real.logb_le_iff_le_rpow (one_lt_D (X := X)) (mul_pos four_pos hxy), mul_comm]
    exact_mod_cast (le_div_iff‚ÇÄ four_pos).mp h.2

