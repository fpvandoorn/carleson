  ¬∑ by_cases hX : GeneralCase f Œ±; swap
    ¬∑ exact lt_of_le_of_lt (measure_mono (subset_univ _)) <| volume_lt_of_not_GeneralCase hf hX hŒ±
    calc volume (support (czApproximation f Œ±))
