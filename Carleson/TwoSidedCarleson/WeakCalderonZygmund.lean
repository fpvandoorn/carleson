    _ ‚â§ (‚à´‚Åª x, ‚ÄñglobalMaximalFunction volume 1 f x‚Äñ‚Çë ^ 2) ^ (1 / 2 : ‚Ñù) := by
      refine rpow_le_rpow ?_ (by norm_num)
