      gcongr
    -- Note: simp, ring_nf, field_simp did not help.
    have heq : ‚Üë(C2_0_5 a) * volume (ball (x1 : X) (D ^ ùî∞ p')) *
      (‚Üë(C6_2_1 a) / (volume (ball (x1 : X) (D ^ ùî∞ p')) * volume (ball (x2 : X) (D ^ ùî∞ p)))) =
